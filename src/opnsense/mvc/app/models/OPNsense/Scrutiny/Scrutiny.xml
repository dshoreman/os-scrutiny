<?xml version="1.0"?>
<model>
    <mount>//OPNsense/Scrutiny</mount>
    <items>
        <settings>
            <collector_version type="TextField" volatile="true">
                <default>-</default>
            </collector_version>
            <smart_version type="TextField" volatile="true">
                <default>-</default>
            </smart_version>
            <api_endpoint type="TextField">
                <required>Y</required>
            </api_endpoint>
            <debug type="BooleanField">
                <default>0</default>
            </debug>
            <host_id type="TextField" />
        </settings>
    </items>
</model>
